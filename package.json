{
  "name": "keo",
  "version": "4.0.0",
  "description": "Plain functions for a more functional Deku approach to creating React components, with functional goodies such as compose, memoize, etc... for free.",
  "scripts": {
    "test": "ava tests/*.test.js --tap | tap-spec",
    "start": "node example/server/default.js",
    "build": "webpack && npm run dev",
    "dev": "browserify example/js/default.js -o example/js/build.js -t [ babelify --presets [ es2015 react ] ]"
  },
  "main": "keo.js",
  "files": [
    "keo.js",
    "redux.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Keo.git"
  },
  "keywords": [
    "functional",
    "react",
    "promises",
    "state",
    "components",
    "functions",
    "compose",
    "pipe",
    "identity",
    "memoize",
    "curry"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Keo/issues"
  },
  "homepage": "https://github.com/Wildhoney/Keo#readme",
  "devDependencies": {
    "array-shuffle": "^1.0.0",
    "ava": "git+https://github.com/sindresorhus/ava.git",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.6.5",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "document-ready-promise": "^3.0.1",
    "enzyme": "^2.0.0",
    "express": "^4.13.4",
    "is-empty-object": "^1.1.1",
    "json-loader": "^0.5.4",
    "node-fetch": "^1.3.3",
    "object-hash": "^1.1.2",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "redux-thunk": "^2.0.1",
    "seamless-immutable": "^5.1.0",
    "sinon": "^1.17.3",
    "starwars": "^1.0.0",
    "tap-spec": "^4.1.1",
    "unique-random-array": "^1.0.0",
    "webpack": "^2.1.0-beta.4"
  },
  "dependencies": {
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-addons-perf": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.5"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "./tests/helpers/setup-browser-env.js"
    ]
  }
}
