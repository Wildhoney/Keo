{
  "name": "keo",
  "version": "2.1.1",
  "description": "Plain functions for a more functional Deku approach to creating React components, with functional goodies such as pipe, memoize, etc... for free.",
  "scripts": {
    "test": "ava tests/*.test.js",
    "start": "node example/server/default.js",
    "prebuild": "rimraf dist && mkdir dist",
    "prepublish": "npm run clean && npm run example && mv dist/* . && rimraf dist",
    "postpublish": "mkdir dist && mv keo.js redux.js strict.js dist/",
    "clean": "rimraf dist && npm run build",
    "build": "webpack",
    "example": "browserify example/js/app.js -o example/js/example.js -t [ babelify --presets [ es2015 react ] ]",
    "watch": "npm run build && watch 'npm run example' example/"
  },
  "main": "keo.js",
  "files": [
    "keo.js",
    "redux.js",
    "strict.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Keo.git"
  },
  "keywords": [
    "functional",
    "react",
    "promises",
    "state",
    "components",
    "functions",
    "compose",
    "pipe",
    "identity",
    "memoize",
    "curry"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Keo/issues"
  },
  "homepage": "https://github.com/Wildhoney/Keo#readme",
  "devDependencies": {
    "ava": "^0.9.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "enzyme": "^1.6.0",
    "express": "^4.13.4",
    "json-loader": "^0.5.4",
    "moment": "^2.11.0",
    "node-fetch": "^1.3.3",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.0",
    "redux": "^3.0.5",
    "rimraf": "^2.5.2",
    "webpack": "^2.0.7-beta"
  },
  "dependencies": {
    "babel-loader": "^6.2.2",
    "es6-weak-map": "^2.0.1",
    "funkel": "^0.2.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "optionalDependencies": {
    "react-redux": "^4.0.0",
    "redux": "^3.0.5"
  }
}
